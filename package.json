{
  "name": "rollup-plugin-assets-replace",
  "version": "0.1.0",
  "description": "Rollup plugin which replaces assets",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "prepare": "npx is-ci || husky install",
    "lint-staged": "lint-staged",
    "lint:eslint": "eslint '**/*.{ts,js}'",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "_prettier": "prettier '**/*.{ts,js}'",
    "lint:prettier": "npm run _prettier -- --check",
    "fix:prettier": "npm run _prettier -- --write",
    "typecheck": "tsc --noEmit",
    "test": "ava",
    "build": "rollup --config rollup.config.ts --configPlugin typescript",
    "clean": "rm -rf ./lib",
    "prebuild": "npm run clean",
    "postbuild": "tsc --emitDeclarationOnly"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sugarshin/rollup-plugin-assets-replace.git"
  },
  "keywords": [
    "rollup",
    "plugin",
    "rollup-plugin",
    "assets",
    "replace"
  ],
  "author": "sugarshin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sugarshin/rollup-plugin-assets-replace/issues"
  },
  "homepage": "https://github.com/sugarshin/rollup-plugin-asses-replace#readme",
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "files": [
      "!**/fixtures/**"
    ]
  },
  "dependencies": {
    "@rollup/pluginutils": "^4.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "8.2.1",
    "@tsconfig/node10": "1.0.8",
    "@types/node": "14.17.5",
    "@typescript-eslint/eslint-plugin": "4.28.3",
    "@typescript-eslint/parser": "4.28.3",
    "ava": "3.15.0",
    "eslint": "7.30.0",
    "husky": "7.0.1",
    "is-ci": "2.0.0",
    "lint-staged": "11.0.1",
    "prettier": "2.3.2",
    "rollup": "2.53.1",
    "ts-node": "10.1.0",
    "tslib": "2.3.0",
    "type-fest": "1.2.2",
    "typescript": "4.3.5"
  },
  "peerDependencies": {
    "rollup": "^2.0.0"
  }
}
